# API Keys for external services
# Replace these placeholder values with your actual API keys

# OpenAI API Key (for LLM responses - GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Redis URL (for caching)
REDIS_URL=redis://localhost:6379

# Optional: Google Gemini API Key (if using Gemini models)
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub Token (for accessing GitHub APIs)
GITHUB_TOKEN=your_github_token_here

# Hugging Face Token (for custom embeddings)
HF_TOKEN=your_huggingface_token_here
